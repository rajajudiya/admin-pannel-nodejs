    display: flex;
    align-items: center;
} */
.delta-1 {
    /* display: flex; */
    /* align-items: center; */
}

.docu-box {
    height: 30px;
    width: 50px;
    color: red;
    background-color: var(--bg-color-2);
    border: 1px solid red;
    border-radius: 20px;
    display: flex;
    margin: 20px 0px 0px 20px;
    padding-left: 10px;
}

