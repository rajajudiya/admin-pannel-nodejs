    <style>
        * {
    margin: 0px;
    padding: 0px;
    box-sizing: border-box;
    font-family: 'Jost';
    list-style: none;
}

:root {
    --heading-color: #262626;
    --text-color-1: #292F36;
    --bg-color-body: #F8F9FA;
    --bg-color-2: #FFFFFF;
    --text-color-1: #333B44;
    --text-color-2: #ECEDEE;
    --btn-color: #102693;
    --font-family: 'Inter';
    --lh: 1.5;
    --fw: 400;
    --fs: 20px;
    --gutter-width: 15px;
}

.w-1 {
    width: 8.33%;
}

.w-2 {
    width: 16.66%;
}

.w-3 {
    width: 25%;
}

.w-4 {
    width: 33.33%;
}

.w-5 {
    width: 41.66%;
}

.w-6 {
    width: 50%;
}

.w-7 {
    width: 58.33%;
}

.w-8 {
    width: 66.66%;
}

.w-9 {
    width: 75%;
}

.w-10 {
    width: 83.33%;
}

.w-11 {
    width: 91.66%;
}

.w-12 {
    width: 100%;
}

.w-1,
.w-2,
.w-3,
.w-4,
.w-5,
.w-6,
.w-7,
.w-8,
.w-9,
.w-10,
.w-11,
.w-12 {
    padding-left: var(--gutter-width);
    padding-right: var(--gutter-width);
}

.container {
    width: 100%;

}

body {
    background-color: #ffffff;
    color: var(--secondary-color);
    font-family: var(--font);
    font-size: var(--fs);
    font-weight: var(--fw);
    line-height: var(--lh);
}

.row {
    display: flex;
    flex-wrap: wrap;

}

.display-flex {
    display: flex;
}

.align-item {
    align-items: center;
}

.flex-end {
    justify-content: flex-end;
}

.spce-between {
    justify-content: space-between;
}

.side-bar {
    width: 280px;
    height: 100vh;
    background-color: var(--bg-color-body);
    position: fixed;
    top: 0;
    left: 0px;
    border-right: 2px dotted grey;
    border-radius: 5px;
    display: flex;
    justify-content: center;
    /* align-items: center; */
    overflow-y: scroll;
    color: #3E4853;
    font-size: 11px;
    font-family: 'Jost', sans-serif;
    font-weight: 800;
}

.side-bar span {
    color: rgb(62, 72, 83);
    font-size: 14px;
}

/* .side-bar ul li span:hover{
    color: blue;
} */
.side-bar svg:hover {
    color: lightblue;
    border: #333B44;
}

.side-bar.close-sider {
    width: 80px;
}

.down {
    position: relative;

}

.down.di-none {
    display: none;
}

.down::before {
    position: absolute;
    content: '\27A7';
    /* right: 5px; */
    left: 8em;
    top: 50%;
    transform: translatey(-50%);
    font-size: 20px;
    font-weight: 500;
}

.down1 {
    position: relative;

}

.down1.di-none {
    display: none;
}

.down1::before {
    position: absolute;
    content: '\2022';
    right: 9em;
    top: 50%;
    transform: translatey(-50%);
    font-size: 20px;
    font-weight: 500;
}

header {
    height: 60px;
    position: fixed;
    top: 0px;
    width: calc(100% - 280px);
    /* left: 0px; */
    margin-left: 280px;
    background-color: var(--bg-color-body);
    padding-top: 14px;
    z-index: 999;
}

header.left {
    margin-left: 80px;
    width: calc(100% - 80px);
    transition: 0.5s all ease;
}

.page-wrapper {
    width: 100%;
    height: 100vh;
    background-color: var(--bg-color-body);
    color: #333B44;
    /* margin-left: 230px; */

}

.logo {
    height: 60px;
    width: 280px;
    /* padding-top: 10px; */
    position: fixed;
    top: 0px;
    left: 0px;
    align-items: center;
    /* justify-content: center; */
    /* margin-bottom: 10px; */
    background-color: var(--bg-color-2);
    z-index: 9;
}

.input input {
    margin-left: 20px;
    height: 30px;
    border-radius: 30px;
    text-align: center;
    border: 1px solid var(--text-color-1);
    font-size: 15px;
}

.search {
    /* padding-left: 100px; */
    bottom: 40px;
    /* top: 50%; */
}

.head-logo img {
    border-radius: 50px;
    height: 40px;
    width: 40px;
    margin-top: -10px;
}

.sidebar-box {
    width: 200px;
    border: 1px solid var(--text-color-1);
    margin-top: 30px;
    border-radius: 20px;
}

.sidebar-box.sidebox-none {
    display: none;
}

.sidebar-box h5 {
    text-align: center;
    font-size: 20px;
    color: var(--heading-color);
}

.sidebar-box h6 {
    text-align: center;
    font-size: 20px;
    color: var(--text-color-1);
    margin-bottom: 20px;
}

.side-btn {
    height: 36px;
    width: 115px;
    background-color: var(--btn-color);
    margin: 0 auto;
    margin-bottom: 20px;

}

.side-btn:hover {
    transform: scale(1.2);
    transition: 1s all ease;
    background-color: blue;
}

.side-btn span {
    text-align: center;
    font-size: 14px;
    /* padding-left: 14px; */
    color: var(--text-color-2);
    padding-top: 3px;
    justify-content: center;
    display: flex;
}

.side .div-icon {
    /* margin-left: 400px; */
    margin-top: 20px;
    list-style: none;
}

.div-icon ul li {
    list-style: none;
    padding-left: 20px;
}

/* header-dropdown */
.drop-down {
    background-color: var(--bg-color-2);
    color: var(--text-color-1);
    position: absolute;
    display: none;
    font-size: 15px;
    border-radius: 20px;
    width: 150px;
    height: 300px;
    line-height: 80px;
    transition: 0.5s all ease-in;
}

/* .position:hover .drop-down{
    visibility: visible;
    opacity: 1;
    transition: 0.5s all ease-in;
}  */
.position {
    position: relative;
}

.toggle-icon.close-sider {
    width: 80px;
}

.sidebar-notify {
    margin-top: 60px;
    /* padding-left: 40px; */
}

.sidebar-notify ul li:hover {
    /* background-color:var(--heading-color); */
    opacity: trans;
}

.sidebar-notify ul li a {
    list-style: none;
    font-size: 17px;
    display: block;
    line-height: 60px;
    color: var(--text-color-1);
    /* padding-left: 10px; */
    text-decoration: none;
}

.delta li span.di-none {
    display: none;
}

::-webkit-scrollbar {
    display: none;
}

.side-dropdown {
    display: none;
    margin-left: 30px;
}

