    background-color: #5968B4;
    border: 1px solid var(--bg-color-body);
    color: var(--text-color-2);
    text-align: center;
    margin-top: 10px;
    margin-left: 20px;
}

.btn-2:hover {
    background-color: #3366FF;
    transform: scale(1.2);
    transition: 0.5s all ease;
}

