}

.revenue,.revenue{
    width: 236px;
    height: 237px;
    box-shadow: 0 14px 28px rgba(0, 0, 0, 0.1), 0 2px 2px rgba(0, 0, 0, 0.1);
    border-radius: 10px;
    padding: 12px;

}
.revenue-c{
    display: flex;
    flex-wrap: wrap;
}

.revenue-c h4{
    color: var(--text-color-2);
